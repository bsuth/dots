! Swap Caps Lock and Control-L
clear Lock
clear Control
keycode 37 = Caps_Lock
keycode 66 = Control_L
add Control = Control_L Control_R
add Lock = Caps_Lock

! Swap Alt_R and Super_R
clear Mod1
clear Mod4
! swapped
keycode 108 = Super_R
keycode 134 = Alt_R
! not swapped
keycode 133 = Super_L
keycode 64 = Alt_L
add Mod1 = Alt_R Alt_L
add Mod4 = Super_R Super_L
