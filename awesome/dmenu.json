{
  "sleep": {
    "exec": "systemctl suspend"
  },
  "reboot": {
    "exec": "/sbin/reboot"
  },
  "poweroff": {
    "exec": "/sbin/poweroff"
  },
  "gpick": {
    "exec": "gpick -s -o | tr -d '\\n' | xclip -sel c",
    "with_shell": true
  },
  "flameshot": {
    "exec": "flameshot gui"
  },
  "inkscape": {
    "exec": "inkscape"
  },
  "discord": {
    "exec": "discord"
  },
  "simplescreenrecorder": {
    "exec": "simplescreenrecorder"
  },
  "firefox": {
    "exec": "firefox-developer-edition"
  },
  "aseprite": {
    "exec": "aseprite"
  },
  "godot": {
    "exec": "godot"
  }
}
