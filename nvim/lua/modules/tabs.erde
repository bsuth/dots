vim.keymap.set('n', '<c-t>', () -> vim.cmd("tabnew { vim.fn.getcwd() }"))
vim.keymap.set('n', '<Tab>', () -> vim.cmd('tabnext'))
vim.keymap.set('n', '<S-Tab>', () -> vim.cmd('tabprevious'))

